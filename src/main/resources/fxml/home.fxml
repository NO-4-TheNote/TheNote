<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.note.HomeController">
   <children>
      <MenuBar fx:id="homeMenu">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem mnemonicParsing="false" text="Close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Edit">
            <items>
              <MenuItem mnemonicParsing="false" text="Delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox prefWidth="200.0" spacing="2.0">
               <children>
                  <ComboBox prefHeight="24.0" prefWidth="200.0" VBox.vgrow="NEVER" />
                  <ListView prefWidth="200.0" VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets top="2.0" />
               </padding>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="200.0">
               <children>
                  <HBox alignment="CENTER" spacing="2.0" VBox.vgrow="NEVER">
                     <children>
                        <TextField HBox.hgrow="ALWAYS" />
                        <Button mnemonicParsing="false" prefHeight="24.0" prefWidth="24.0" textAlignment="CENTER" textOverrun="CLIP" />
                     </children>
                     <padding>
                        <Insets bottom="2.0" left="2.0" right="2.0" />
                     </padding>
                  </HBox>
                  <ListView prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets top="2.0" />
               </padding></VBox>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="2.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField prefHeight="24.0" />
                  <TextArea VBox.vgrow="ALWAYS" />
               </children>
               <padding>
                  <Insets top="2.0" />
               </padding></VBox>
         </children>
      </HBox>
   </children>
</VBox>
